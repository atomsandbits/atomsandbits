    "sprintf-js": "^1.1.1",
    "winston": "^3.0.0-rc1"
  },
  "devDependencies": {
    "ajv": "^6.1.1",
    "babel-eslint": "^8.2.2",
    "chai": "^4.1.2",
    "chromedriver": "^2.35.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.0",
    "scripty": "^1.7.2",
    "selenium-webdriver": "^3.6.0"
